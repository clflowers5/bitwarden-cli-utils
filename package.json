{
  "name": "bitwarden-cli-utils",
  "version": "0.0.5",
  "description": "A TypeScript / JavaScript wrapper for the bitwarden-cli. Simplifying programmatic credential management.",
  "keywords": [
    "bitwarden",
    "cli",
    "password",
    "password manager",
    "credentials"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc --declaration",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "author": "Chris Flowers",
  "license": "ISC",
  "dependencies": {
    "@bitwarden/cli": "^1.4.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/node": "^10.12.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": "^.+\\.spec\\.(ts|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
